/*表格*/
.table-container{
    margin-top: 15px;
    border-radius: 6px;
    background-color: rgba(255,255,255,0.1);
    overflow: hidden;

}
.table-container2{
    margin-bottom: 15px;
}
.table-container .thead-bg{
    width: 100%;
    height: 30px;
    background-color: rgba(71,190,252,0.25);
}
.table-container .table-box{
    text-align: center;
    margin-top: -30px;
}
.table-container .table{
    margin-bottom: 0;
    table-layout:fixed;
    border-collapse:collapse;
}
.table-container .table thead{
    border: none;
}
.table-container .table th{
    font-weight: normal;
    white-space:nowrap;
    text-align: center;
    border: none;
    overflow:hidden;
    text-overflow:ellipsis;
}
.table-container .table-condensed>tbody>tr>td{
    line-height: 28px;
    border-top: none;
    white-space:nowrap;
    border-bottom: 1px solid #44515a;
    overflow:hidden;
    text-overflow:ellipsis;
}
.table-container .table-condensed>tbody>tr:nth-last-child(1) td{
    border-bottom: none;
}
.table-container .table-condensed>tbody>tr>td:nth-child(1){
    font-size: 16px;
    font-weight: 400;
    color: #dfc612;
}

.error {
    color: rgb(255,0,0);
}
input[type=checkbox], input[type=radio] {
    margin: 0;
}
.checkbox input[type=checkbox], .checkbox-inline input[type=checkbox], .radio input[type=radio], .radio-inline input[type=radio] {
    margin-top: 4px;
}

.z_jurisdiction tr td{
    color: #fff !important;
    font-size: 14px !important;
}
.z_jurisdiction {
    table-layout: inherit !important;
}
/*表格 end*/
