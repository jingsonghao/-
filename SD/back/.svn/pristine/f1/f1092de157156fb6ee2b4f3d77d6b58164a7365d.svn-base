<script>
    $("#budget").slider({
        min: 0,
        max: 100,
        step: 1,
        value: $('input[name="budget"]').val(),
        slide: function( event, ui ) {
            if(ui.value<25){
                $('input[name="budget"]').val("差");
            }else if(ui.value<50){
                $('input[name="budget"]').val("中");
            }else if(ui.value<75){
                $('input[name="budget"]').val("良");
            }else{
                $('input[name="budget"]').val("优");
            }
        }
    });
</script>

