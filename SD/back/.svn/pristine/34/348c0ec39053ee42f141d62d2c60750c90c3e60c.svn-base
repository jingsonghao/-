<script>
    $("#budget").slider({
        min: 0,
        max: 100,
        step: 1,
        value: $('input[name="budget"]').val(),
        slide: function( event, ui ) {
            if(ui.value<25){
                $('input[name="budget"]').val("正常");
            }else if(ui.value<50){
                $('input[name="budget"]').val("一般");
            }else if(ui.value<75){
                $('input[name="budget"]').val("严重");
            }else{
                $('input[name="budget"]').val("重大");
            }
        }
    });
</script>

</body>

</html>